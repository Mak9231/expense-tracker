<%= paginator.render do %>




<nav>
  <ul class="pagination justify-content-center">
    <% if paginator.prev_page %>
      <li class="page-item">
        <%= link_to raw("&laquo;"), url_for(page: paginator.prev_page), class: "page-link" %>
      </li>
    <% else %>
      <li class="page-item disabled"><span class="page-link">&laquo;</span></li>
    <% end %>

    <% paginator.each_page do |page| %>
      <% if page %>
        <li class="page-item <%= 'active' if page == current_page %>">
          <%= link_to page, url_for(page: page), class: "page-link" %>
        </li>
      <% else %>
        <li class="page-item disabled"><span class="page-link">â€¦</span></li>
      <% end %>
    <% end %>

    <% if paginator.next_page %>
      <li class="page-item">
        <%= link_to raw("&raquo;"), url_for(page: paginator.next_page), class: "page-link" %>
      </li>
    <% else %>
      <li class="page-item disabled"><span class="page-link">&raquo;</span></li>
    <% end %>
  </ul>
</nav>
